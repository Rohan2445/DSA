
#include <iostream>

using namespace std;

int main() {
    int numbers[5];
    int max_num;

    for (int i = 0; i < 5; i++) {
        cout << "Enter integer " << i + 1 << ": ";
        cin >> numbers[i];
    }

    max_num = numbers[0];

    for (int i = 1; i < 5; i++) {
        if (numbers[i] > max_num) {
            max_num = numbers[i];
        }
    }

    cout << "The maximum number is: " << max_num << endl;

    return 0;
}
